{
  "name": "lusamine",
  "version": "1.0.3",
  "private": true,
  "homepage": "https://bakuzan.github.io/lusamine/",
  "dependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.2.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "meiko": "file:../meiko/build",
    "node-sass": "^4.10.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "react": "^16.8.2",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dnd-touch-backend": "^0.8.0",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.5"
  },
  "lint-staged": {
    "{,src,tools}/**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "scripts": {
    "precommit-msg": "echo 'Precommit checks...'",
    "lint": "lint-staged",
    "start": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts start",
    "build": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build",
    "scrape-data": "node ./tools/index.js"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "gh-pages": "^1.1.0",
    "request": "^2.88.0"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
