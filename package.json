{
  "name": "lusamine",
  "version": "1.5.0",
  "private": true,
  "homepage": "https://bakuzan.github.io/lusamine/",
  "dependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.2.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.6",
    "meiko": "file:../meiko/build",
    "nano-css": "^5.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dnd-touch-backend": "^0.8.1",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0"
  },
  "lint-staged": {
    "{,src,tools}/**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit-msg && npm run lint"
    }
  },
  "scripts": {
    "precommit-msg": "echo 'Precommit checks...'",
    "lint": "lint-staged",
    "start": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts start",
    "build": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build",
    "scrape": "node ./tools/index.js"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "gh-pages": "^2.0.1",
    "request": "^2.88.0"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
