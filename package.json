{
  "name": "lusamine",
  "version": "0.12.0",
  "private": true,
  "homepage": "https://bakuzan.github.io/lusamine/",
  "dependencies": {
    "classnames": "^2.2.5",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "meiko": "file:../meiko",
    "node-sass": "^4.10.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "react": "^16.6.3",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.1"
  },
  "lint-staged": {
    "{,src,tools}/**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "scripts": {
    "precommit-msg": "echo 'Precommit checks...'",
    "lint": "lint-staged",
    "start": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts start",
    "build": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build",
    "scrape-data": "node ./tools/index.js"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "gh-pages": "^1.1.0",
    "node-sprite-generator": "^0.10.2",
    "request": "^2.85.0"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
