{
  "name": "lusamine",
  "version": "1.8.0",
  "private": true,
  "homepage": "https://bakuzan.github.io/lusamine/",
  "dependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.2.6",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "meiko": "file:../meiko/build",
    "nano-css": "^5.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dnd": "^7.6.0",
    "react-dnd-html5-backend": "^7.6.0",
    "react-dnd-touch-backend": "^0.8.3",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "lint-staged": {
    "{,src,tools}/**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit-msg && npm run lint"
    }
  },
  "scripts": {
    "precommit-msg": "echo 'Precommit checks...'",
    "lint": "lint-staged",
    "start": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts start",
    "build": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build",
    "scrape": "node ./tools/index.js",
    "scrape-pokedex": "node ./tools/regionalPokedex/index.js"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "gh-pages": "^2.0.1",
    "minimist": "^1.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
